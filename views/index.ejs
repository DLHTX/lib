<script>
    $(document).ready(function() {
         //手机端隐藏项目导航 PC端显示
        console.log($(window).width())
        if($(window).width()>720){
            $.fn.fullpage({
                slidesColor: ['#1bbc9b', '#4BBFC3', '#7BAABE', '#f90','#4BBFC3','#1bbc9b'],
                anchors: ['page1', 'page2', 'page3', 'page4','page5','page6'],
                menu: '#menu',
                navigation:true,
                scrollingSpeed:300,
                verticalCentered:true,
                loopBottom:true,
                paddingTop:"65px"
            })
       }


        else {
            $.fn.fullpage({
                slidesColor: ['#1bbc9b', '#4BBFC3', '#7BAABE', '#f90'],
                anchors: ['page1', 'page2', 'page3', 'page4'],
                menu: '#menu',
                navigation:false,
                scrollingSpeed:700,
                verticalCentered:true,
                loopBottom:true,
                paddingTop:"20px"
            })
        }
    });

    //page1
    $(document).ready(function () {
        $(".shumu-shrink-nav").css("display","none")
        $(".shumu").click(function () {
            $("#nav1").fadeToggle(300)
            $("#nav2").fadeOut(300);
        });
        $("#nav1").find("span").click(function () {
            var oHtml=$(this).html();
            $(".shumu .btn-head").html(oHtml)
        })



        $(".timing").click(function () {
            $("#nav2").fadeToggle(300)
            $("#nav1").fadeOut(300);
        })

        $("#nav2").find("span").click(function () {
            var oHtml=$(this).html();
            $(".timing .btn-head").html(oHtml)
        })


        // 检索
        $(".search-box input").focus(function () {
            console.log('aaa')
            $('.searchBtn').css({
                background:'rgb(220,20,60)',
                transition:'all .3s'
            })
        })

        $(".search-box input").blur(function () {
            console.log('aaa')
            $('.searchBtn').css({
                background:'rgb(73,197,182)',
                transition:'all .3s'
            })
        })



    })

    //page2


    $(document).ready(function(){
        var pos = 0;
        var totalSlides = $('#wrapper ul li').length;
        var sliderWidth = $('#wrapper').width();
        $('#wrapper ul#slider').width(sliderWidth*totalSlides);/* ul总宽度*/
        $('#next').click(function(){
            slideRight();
        });
        $('#previous').click(function(){
            slideLeft();
        });
        var autoSlider = setInterval(slideRight, 3000);
        $.each($('#wrapper ul li'), function() {
            //create a pagination
            var li = document.createElement('li');
            $('#pagination-wrap ul').append(li);
        });
        countSlides();
        pagination();
        $('#wrapper').hover(
            function(){ $(this).addClass('active'); clearInterval(autoSlider); },
            function(){ $(this).removeClass('active'); autoSlider = setInterval(slideRight, 3000); }
        );
        function slideLeft(){
            pos--;
            if(pos==-1){ pos = totalSlides-1; }
            $('#wrapper ul#slider').animate({left: -(sliderWidth*pos)+"px"},500);

            //*> optional
            countSlides();
            pagination();
        }
        function slideRight(){
            pos++;
            if(pos==totalSlides){ pos = 0; }
            $('#wrapper ul#slider').animate({left: -(sliderWidth*pos)+"px"},500);

            //*> optional
            countSlides();
            pagination();
        }
        function countSlides(){
            $('#counter').html(pos+1 + ' / ' + totalSlides);
        }

        function pagination(){
            $('#pagination-wrap ul li').removeClass('active');
            $('#pagination-wrap ul li:eq('+pos+')').addClass('active');
        }


        // news-list

        $(".news-group-header span").click(
            function () {
                $( this ).addClass("active").siblings().removeClass("active");
                var index= $( this ).index();
                $(".news-group-list>div").eq(index).slideDown().siblings().hide();
            }
        )
    });

    //page3
    $(document).ready(function () {
        // page3  news-book-bottom
        $(".youlike .news-book-header span").click(function () {
            $( this ).addClass("active").siblings().removeClass("active");
            var index= $( this ).index();
            $(".youlike").find(".list-first-box").eq(index).show().siblings().hide()
        })

        var oHeight = $(".paihang").find("li").eq(1).height();
        var oLenght= $(".paihang ul").children('li').length;
        var m= $(".paihang ul").outerHeight(true);
        var x=-oHeight*oLenght*30;

        function Move () {
            $(".paihang ul").animate({
                marginTop: -m
            },22000,function () {
                var s=Math.abs(parseInt(($(this).css("margin-top"))))
                if(s >= m){
                    // $(this).find("li").slice(0, 15).appendTo($(this));//slice选取一段元素
                    $(this).css("margin-top", 0);
                }
            })
        }
        Move();
        // HOVER  停止当前动画
        $(".paihang ul").hover(function(){
            $(this).stop();
        },function () {
            Move();
        })
        // // news-book
        //current pos1ition
        var pos1 = 0;
        //number of slides
        var totalSlides1 = $('.news-book-list-box ul li').length;

        //get the slide width
        var sliderWidth1 = $('.news-book-list-box').width();

        /*****************
         BUILD THE SLIDER
         *****************/
        //set width to be 'x' times the number of slides
        $('.news-book-list-box ul').width(sliderWidth1*totalSlides1);/* ul总宽度*/
        //next slide
        $('.left-btn').click(function(){

            slideRight1();
        });

        //previous slide
        $('.right-btn').click(function(){
            slideLeft1();

        });

        var autoSlider1= setInterval(slideRight1, 3000);

        //for each slide
        $.each($('.news-book-list-box ul li'), function() {
            //create a pagination1
            var li = document.createElement('li');
            $('.news-book-header-ul').append(li);
        });

        //counter
        // countSlides11();

        //pagination1
        pagination1();

        //hide/show controls/btns when hover
        //pause automatic slide when hover
        $('.news-book').hover(
            function(){ $(this).addClass('active1');

                clearInterval(autoSlider1); },
            function(){ $(this).removeClass('active1'); autoSlider1= setInterval(slideRight1, 3000); }
        );





        /***********
         SLIDE LEFT  定义左右控件
         ************/
        function slideLeft1(){
            pos1--;
            if(pos1==-1){ pos1 = totalSlides1-1; }
            $('.news-book-list-box ul.news-book-list-ul').animate({left: -(sliderWidth1*pos1)+"px"},500);

            //*> optional
            // countSlides1();
            pagination1();
        }


        /************
         SLIDE RIGHT
         *************/
        function slideRight1(){
            pos1++;
            if(pos1==totalSlides1){ pos1 = 0; }
            $('.news-book-list-box ul.news-book-list-ul').animate({left: -(sliderWidth1*pos1)+"px"},500);

            //*> optional
            // countSlides1();
            pagination1();
        }




        /************************
         //*> OPTIONAL SETTINGS
         ************************/
        // function countSlides1(){
        //     $('#counter').html(pos1+1 + ' / ' + totalSlides1);
        // }

        function pagination1(){
            $('.news-book-header-ul li').removeClass('active1');
            // $('.news-book-header-ul li:eq('-pos1-')').addClass('active1');
            $('.news-book-header-ul li').eq(pos1-1).addClass('active1');
        }

    });

    //page4

    $(document).ready(function () {
        $(".main").find("a").click(function () {
            $( this ).addClass("list-active").siblings().removeClass("list-active");
            var x = $(this).index();
            $(".buttom>div").eq(x+1).css({display:"block"}).siblings().css({display:"none"})
        })

    })
    //nav
    $(document).ready(function () {
        $(window).resize(function () {          //当浏览器大小变化时
            if($(window).width()>720){
                $(".shrink-nav").hide();
            }
        })
        $(".shrink").click(function () {
            $(this).toggleClass("shrink-click shrink-out");
            $(".shrink-nav").slideToggle();
        })
        $(".shrink-nav a").each(function( index ) {
            $( this ).css({'animation-delay': (index/10)+'s'});  /*animation-delay 动画延迟执行*/
        });
    })

    //index
    $(document).ready(function () {
        var oUl=$("#news-ul")
        var height;
        height=$(".news-box").outerHeight();
        $(window).resize(function () {
            height=$(".news-box").outerHeight();
            // return height;
        })
        var num = oUl.find("li").length;
        var i=0;
        setInterval(function () {
            i++;
            if(i==num){i=0}
            var top=-i*height;
            oUl.animate({top:top})
        },3000)

    })
</script>
</body>
