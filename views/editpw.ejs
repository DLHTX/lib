<body>
<div class="center-container">
    <!--header-->
    <div class="header-w3l">
        <h1> Digital Library Editpassword</h1>
    </div>
    <!--//header-->
    <div class="main-content-agile">
        <div class="sub-main-w3">
            <div class="wthree-pro">
                <h2>Editpassword Quick</h2>
            </div>

            <div class="errormsg"></div>

            <div class="pom-agile">
                <input placeholder="UserName" name="Name" class="user" type="email" required="" id="editusername">
                <span class="icon1"><i class="fa fa-user" aria-hidden="true"></i></span>
            </div>
            <div class="pom-agile">
                <input  placeholder="NewPassword" name="Password" class="pass" type="password" required=""  id="editpassword">
                <span class="icon2"><i class="fa fa-unlock" aria-hidden="true"></i></span>
            </div>

            <div class="sub-w3l">
                <div class="right-w3l editpassword-btn">
                    <input type="submit" value="Editpassword" >
                </div>
            </div>

        </div>
    </div>
    <!--//main-->
    <!--footer-->
    <div class="footer">
        <p>&copy; 2017 Online Login Form. All rights reserved </p>
    </div>
</div>
